        position: fixed;
        z-index: 99999999;
        bottom: 20px;
        left: 50%;
        width: auto;
    }
}

.ot-cookie-settings.show {
    left: 0%;
    animation-name: show-bottom-cookie-settings;
    animation-timing-function: linear;
    animation-fill-mode: forwards;
    animation-duration: 10s;
    display: block;
    visibility: hidden;
}


@media (min-width: 768px) {
    .ot-cookie-settings.show {
        left: 50%;
