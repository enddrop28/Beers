    }
}

@keyframes show-bottom-cookie-settings {
    0% {
        opacity: 0;
        visibility: visible;
    }

    15% {
        opacity: 1;
        visibility: visible;
    }

    50% {
        opacity: 1;
        visibility: visible;
    }

    80% {
        opacity: 1;
        visibility: visible;
    }

